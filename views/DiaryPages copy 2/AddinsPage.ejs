<html>
    <head>
        <style>
           body {
               font-family: Arial, sans-serif;
               background-color: #FFFFFF;
           }
           .st5InsideCoverdiv {
               width: 2175px;
               height: 2925px;
               background: #FFF;
               position: relative;
               display: flex;
               flex-direction: row;
               justify-content: center;
               break-after: page;
           }
           .st5Coverdiv {
               display: flex;
               flex-direction: row;
               width: 100%;
               height: 100%;
           }
           .st5page {
               width: 100%;
               position: relative;
               background-color: #FFF;
               text-align: center;
           }
           .leftpage .st5page{
                margin: 90px 213px 90px 90px
            }
            .rightpage .st5page{
                margin: 90px 90px 90px 213px
            }
           .PageSubImages {
               width: 100%;
               height:  100%
           }

   
       </style>
    </head>
    <body>
        
        <% if(Data.Page=="Left"){ %>
        <div class="st5InsideCoverdiv leftpage">
            <div class="st5Coverdiv">
              <div  class="st5page">
                <img class='PageSubImages' src="<%= ImageArr.left_image %>"  alt=''/>
              </div>
            </div>
        </div>
        <% }else{ %>
        <div class="st5InsideCoverdiv rightpage">
            <div class="st5Coverdiv">
                <div class="st5page">
                    <img class='PageSubImages' src="<%= ImageArr.right_image %>"  alt=''/>
                </div>
            </div>
        </div> 
        <% } %>
    </body>
</html>