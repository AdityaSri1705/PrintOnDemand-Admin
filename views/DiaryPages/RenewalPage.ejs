<html>
    <head>
        <style>
           body {
               font-family: Arial, sans-serif;
               background-color: #FFFFFF;
           }
           .RenewalInsideCoverdiv {
               width: 2175px;
               height: 2925px;
               background: #FFF;
               position: relative;
               display: flex;
               flex-direction: row;
               justify-content: center;
               break-after: page;
           }
           .RenewalCoverdiv {
               display: flex;
               flex-direction: row;
               width: 100%;
               height: 100%;
               margin: 0;
               position: relative;
           }
           .Renewalpage {
               width: 100%;
               position: relative;
               background-color: #FFF;
           }
            .leftpage .Renewalpage{
                margin: 90px 213px 90px 90px
            }
            .rightpage .Renewalpage{
                margin: 90px 90px 90px 213px
            }
           .RenewalPageSubImages{
                width: 100%;
                height: 100%;
           }
       </style>
    </head>
    <body>
        
        <% if(Data.Page=="Left"){ %>
        <div class="RenewalInsideCoverdiv leftpage">
            <div class="RenewalCoverdiv">
              <div  class="Renewalpage">
                <img class='RenewalPageSubImages' src="<%= ImageArr.left_image %>"  alt=''/>
              </div>
            </div>
        </div>
        <% }else{ %>
        <div class="RenewalInsideCoverdiv rightpage">
            <div class="RenewalCoverdiv">
                <div class="Renewalpage">
                    <img class='RenewalPageSubImages' src="<%= ImageArr.right_image %>"  alt=''/>
                </div>
            </div>
        </div> 
        <% } %>
    </body>
</html>