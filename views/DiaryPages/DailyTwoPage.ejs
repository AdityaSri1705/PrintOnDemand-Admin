            }

            .TopLeft {
                position: absolute;
                top:108pt;
                left: 0;
                width: 45%;
                height: 6%;
            }
            .TopLeftDText {
                top: 24pt;
                left: 45pt;
                font-size: 42pt !important;
            }
            .TopRight {
                top: 108pt;
                right: 0;
                width: 45%;
