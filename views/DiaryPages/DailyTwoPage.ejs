            }

            .cdate{
                display: flex;
                align-items: baseline;
            }
            .DailyDateBTitle{
                font-size: 36pt !important;
                color: #212121;
                font-family: JosefinSans !important;
                font-style: normal;
                font-weight: 700 !important;
                line-height: 1 !important;
                text-transform: uppercase; 
                margin-right: 15pt;
            }
            .DailyDateSTitle{
                font-size:  30pt !important;
                color: #212121;
                font-family: JosefinSans !important;
                font-style: normal;
                font-weight: 700;
                line-height: 1.2 !important;
            }
            .Holiday{
               font-size:  27pt !important;
               color: #212121;
               font-family: JosefinSans !important;
               font-style: normal;
               font-weight: 400;
               line-height: 1.2 !important;
           }
           .DTQuoteContainer{
                display: flex;
                justify-content: inherit;
                align-items: center;
                flex-direction: column;
                padding: 15pt;
                text-align: center;
                position: absolute;
                left: 0;
                top: 0;
                width: 100%;
                height: 100%;
                background-color: #FFF;
            }

            .DTQuoteContainer .QuoteText{
                font-size: 36pt;
                font-family: JosefinSans !important;
                line-height: 1.2
            }
            .DTQuoteContainer .author{
                font-size: 36pt;
                font-family: PTSans !important;
                line-height: 1;
                padding-top: 15pt;
            }

            .DTQuoteContainer.RefQuote .QuoteSubContainer{
                position: absolute;
                top: 50%;
                text-align: center;
                background: #FFF;
                height: 50%;
            }
   
       </style>
    </head>
    <body>
        <%   
        const DailyQouteImage = (tab) => {
            // Get the index for the current type
            const type = 3;
            const currentIndex = quoteIndices[type];

            // Get the next quote of the current type
            const newQuoteList = quoteList.filter((item) => item.type === type);
            var quote = newQuoteList[currentIndex];

            // Increment the index for the current type
            quoteIndices[type]++;

            // Reset the index if it exceeds the length of quotes of that type
            if (quoteIndices[type] >= newQuoteList.length) {
                quoteIndices[type] = 0;
            }

            return `<div class='DTQuoteContainer QuoteOnly'><div class='QuoteText'>${quote.quote_text}</div><div class='author'>${quote.author}</div></div>`;
        }
        %>
        <% if(Data.Page=="Left"){ %>
        <div class="step3Container leftpage">
            <div class="st3CoverBox">
              <div  class="st3page">

                <div class="DailyTB-B0">
                    <% if(Data.selectedWeeks.includes(99)){%>
                        <img class='FullWidthImg weekquater' src="<%= ImageArr[Data.WeeksQuaterImgId].left_image %>" alt=''/>
                    <% }%>
                </div>
                <div class="DailyTB-B1">
                    <div class="cdate">
                      <div class="DailyDateBTitle"><%= Data.currentDay %></div>
                      <div class="DailyDateSTitle" ml={1}><%= Data.currentMonthDate %></div>
                    </div>
                    <div class="Holiday">
                      <%= Data.Holidays.map(holiday => holiday.title).join(', ') %>
                    </div>
                </div>
                <div class="DailyTB-B2">
                    <% if(Data.selectedTasks.includes(100)){%>
                        <img class='FullWidthImg preweek' src="<%= ImageArr[100].left_image %>" alt=''/>
                    <% }%>        
                </div>
                <div class="DailyTB-B3">
                    <img class='PageSubImages' src="<%= ImageArr[Data.taskImageId].left_image %>" alt=''/>
                </div>
                <% if(Data.dailySchedule!=97){%>
                <div class="DailyTB-B4">
                <img class='PageSubImages' src="<%= ImageArr[Data.dailySchedule].left_image %>" alt=''/>
                </div>
                <% }%>       
                <div class="<%= (Data.dailySchedule==97)? 'DailyTB-B5 shiftup':'DailyTB-B5'; %>">
                    <% if(Data.dailySchedule!=97 && Data.selectedTime>0){%>
                        <img class="PageSubImages" src="<%= ImageArr[Data.selectedTime].left_image %>" alt=''/>
                    <% }%>
                    <% if(Data.dailySchedule==97 && Data.selectedTime>0){%>
                        <img class="PageSubImages" src="<%= ImageArr[Data.selectedTime].right_image %>" alt=''/>
                    <% }%>
                </div>

              </div>
            </div>
        </div>
        <% }else{ %>
        <div class="step3Container rightpage">
            <div class="st3CoverBox">
                <div class="st3page">

                    <div class="DailyTB-B6">
                        <% if(Data.selectedWeeks.includes(98)){
                             %>
                             <%- DailyQouteImage(98); %>
                            
                        <% }%>
                    </div>
                    <div class="<%= (Data.rightCustomRadio1==116)? 'DailyTB-B7 LineContinue':'DailyTB-B7'; %>" >
                        <img class='FullWidthImg' src="<%= ImageArr[Data.rightCustomRadio1].right_image %>" alt=''/>
                        <% if((Data.rightCustomRadio1 === 113 || Data.rightCustomRadio1===114 || Data.rightCustomRadio1===115) &&  Data.rightCustomRadio1Text!=""){%>
                            <div class="DailyTwoRightText ctext-<%=Data.rightCustomRadio1 %>"><%= Data.rightCustomRadio1Text %></div>
                        <% } %>
                    </div>
                    <div class="<%= (Data.rightCustomRadio2==116)? 'DailyTB-B8 LineContinue':'DailyTB-B8'; %>" >
                        <img class='FullWidthImg' src="<%= ImageArr[Data.rightCustomRadio2].right_image %>" alt=''/>
                        <% if((Data.rightCustomRadio2 === 113 || Data.rightCustomRadio2===114 || Data.rightCustomRadio2===115) &&  Data.rightCustomRadio2Text!=""){%>
                            <div class="DailyTwoRightText ctext-<%=Data.rightCustomRadio2 %>"><%= Data.rightCustomRadio2Text %></div>
                        <% } %>
                    </div>
                    <div class="<%= (Data.rightCustomRadio3==116)? 'DailyTB-B9 LineContinue':'DailyTB-B9'; %>" >
                        <img class='FullWidthImg' src="<%= ImageArr[Data.rightCustomRadio3].right_image %>" alt=''/>
                        <% if((Data.rightCustomRadio3 === 113 || Data.rightCustomRadio3===114 || Data.rightCustomRadio3===115) &&  Data.rightCustomRadio3Text!=""){%>
                            <div class="DailyTwoRightText ctext-<%=Data.rightCustomRadio3 %>"><%= Data.rightCustomRadio3Text %></div>
                        <% } %>
                    </div>
                    <div class="<%= (Data.rightCustomRadio4==116)? 'DailyTB-B10 LineContinue':'DailyTB-B10'; %>" >
                        <img class='FullWidthImg' src="<%= ImageArr[Data.rightCustomRadio4].right_image %>" alt=''/>
                        <% if((Data.rightCustomRadio4 === 113 || Data.rightCustomRadio4===114 || Data.rightCustomRadio4===115) &&  Data.rightCustomRadio4Text!=""){%>
                            <div class="DailyTwoRightText ctext-<%=Data.rightCustomRadio4 %>"><%= Data.rightCustomRadio4Text %></div>
                        <% } %>
                    </div>
                    <div class="<%= (Data.rightCustomRadio5==116)? 'DailyTB-B11 LineContinue':'DailyTB-B11'; %>" >
                        <img class='FullWidthImg' src="<%= ImageArr[Data.rightCustomRadio5].right_image %>" alt=''/>
                        <% if((Data.rightCustomRadio5 === 113 || Data.rightCustomRadio5===114 || Data.rightCustomRadio5===115) &&  Data.rightCustomRadio5Text!=""){%>
                            <div class="DailyTwoRightText ctext-<%=Data.rightCustomRadio5 %>"><%= Data.rightCustomRadio5Text %></div>
                        <% } %>
                    </div>

                    
                </div>
            </div>
        </div>
        <% } %>
    </body>
</html>