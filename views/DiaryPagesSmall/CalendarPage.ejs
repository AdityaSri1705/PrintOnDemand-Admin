<html>
    <head>
        <style>
           body {
               font-family: Arial, sans-serif;
               background-color: #FFFFFF;
           }
           .CalInsideCoverdiv {
               width: 696px;
               height: 936px;
               background: #FFF;
               position: relative;
               display: flex;
               flex-direction: row;
               justify-content: center;
               break-after: page;
           }
           .CalCoverdiv {
               display: flex;
               flex-direction: row;
               width: 100%;
               height: 100%;
               margin: 0;
               position: relative;
           }
           .Calpage {
               width: 100%;
               position: relative;
               background-color: #FFF;
           }
           .leftpage .Calpage{
                margin: 28px 66px 28px 28px
            }
            .rightpage .Calpage{
                margin: 28px 28px 28px 66px
            }
           .CalPageSubImages{
                width: 100%;
                height: 100%;
           }
     
   
       </style>
    </head>
    <body>
        
        <% if(Data.Page=="Left"){ %>
        <div class="CalInsideCoverdiv leftpage">
            <div class="CalCoverdiv">
              <div  class="Calpage">
                <img class='CalPageSubImages' src="<%= ImageArr.left_image %>"  alt=''/>
              </div>
            </div>
        </div>
        <% }else{ %>
        <div class="CalInsideCoverdiv rightpage">
            <div class="CalCoverdiv">
                <div class="Calpage">
                    <img class='CalPageSubImages' src="<%= ImageArr.right_image %>"  alt=''/>
                </div>
            </div>
        </div> 
        <% } %>
    </body>
</html>